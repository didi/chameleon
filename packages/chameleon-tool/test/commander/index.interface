<script cml-type="interface">
interface ReplaceInterface {
  getMsg(msg: String): String;
}

</script>

<script cml-type="web">

class Method implements ReplaceInterface {
  getMsg(msg) {
    return 'web:' + msg;
  }
}

export default new Method();
</script>

